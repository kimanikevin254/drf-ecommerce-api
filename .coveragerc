# .coveragerc
[run]
source = .
omit = 
    */env/*
    */.venv/*
    */migrations/*
    */settings/*
    manage.py
    */tests.py
    */test_*.py
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\(Protocol\):
    @(abc\.)?abstractmethod

show_missing = True
precision = 2

[html]
directory = htmlcov